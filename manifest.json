{
  "name": "DocSend Capture",
  "version": "0.2",
  "description": "Capture DocSend document",
  "permissions": [
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches":["<all_urls>"],
    "js":["content.js"],
    "run_at": "document_end"
  }],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
